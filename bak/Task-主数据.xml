<?xml version="1.0" encoding="utf-8"?>
<JobOption xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Id>7</Id>
  <Name>主数据</Name>
  <CodePath>
    <string>%主数据%</string>
    <string>%项目库%</string>
  </CodePath>
  <SlnFiles>
    <string>%主数据%\主数据系统.sln</string>
    <string>%项目库%\项目库.sln</string>
  </SlnFiles>
  <Actions>
    <Action Type="ClearBinObj" />
    <Action Type="Tfs" />
    <Action Type="ExecuteFile">
      <item>%主数据%\00_Pull.cmd</item>
      <item>%项目库%\00_Pull.cmd</item>
    </Action>
    <Action Type="SetProjecctRule" />
    <Action Type="MsBuild" />
    <Action Type="ResetProjecctRuleTask" />
    <Action Type="ExecuteCommnad">
      <item>copy CustomizeCheckerAssemblyNames.txt  %主数据%\00_根目录\bin\ /y</item>
      <item>copy CustomizeCheckerAssemblyNames.txt  %项目库%\00_根目录\bin\ /y</item>
    </Action>
    <Action Type="AssemblyScan">
      <item>sln=%主数据%\主数据系统.sln;bin=%主数据%\00_根目录\bin</item>
      <item>sln=%项目库%\项目库.sln;bin=%项目库%\00_根目录\bin</item>
    </Action>
    <Action Type="CodeScan" />
    <Action Type="ProjectScan" />
    <Action Type="VsRuleScan" />
    <Action Type="DatabaseScan"></Action>
    <Action Type="ExceptionLog"></Action>
    <Action Type="PerformanceLog"></Action>
    <Action Type="CodeCover" />
    <Action Type="UploadResult"></Action>
    <Action Type="UploadClientLog"></Action>

  </Actions>
</JobOption>