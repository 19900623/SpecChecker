<?xml version="1.0" encoding="utf-8"?>
<JobOption xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Id>5</Id>
  <Name>集成平台</Name>
  <Branch>
      <DbLocation />
      <MongoLocation />
      <ExceptionAlertEmail>wangsy@mysoft.com.cn</ExceptionAlertEmail>
      <IgnoreRules>SPEC:C00005;SPEC:C00021</IgnoreRules>
	  <SubSystems>MIP</SubSystems>
    </Branch>
  <CodePath>
    <string>%MIP%</string>
  </CodePath>
  <SlnFiles>
    <string>%MIP%\Mysoft.Mip.sln</string>
  </SlnFiles>
  <Actions>
    <Action Type="ClearBinObj" />
    <Action Type="Tfs" />
    <Action Type="SetProjecctRule" />
    <Action Type="MsBuild" />
    <Action Type="ResetProjecctRuleTask" />
    <Action Type="AssemblyScan">
      <item>sln=;bin=%MIP%\05 工具\CodeScan\bin</item>
    </Action>
    <Action Type="CodeScan" />
    <Action Type="ProjectScan" />
    <Action Type="VsRuleScan" />
    <Action Type="DatabaseScan"></Action>
    <Action Type="ExceptionLog"></Action>
    <Action Type="PerformanceLog"></Action>
    <Action Type="CodeCover" />
    <Action Type="UploadResult"></Action>
    <Action Type="UploadClientLog"></Action>
  </Actions>
</JobOption>